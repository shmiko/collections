<h1>Listing artists</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Artist type id</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artists.each do |artist| %>
  <tr>
    <td><%= artist.Name %></td>
    <td><%= artist.Artist_Type_ID %></td>
    <td><%= artist.Created %></td>
    <td><%= artist.Updated %></td>
    <td><%= artist.Created_Person_ID %></td>
    <td><%= artist.Updated_Person_ID %></td>
    <td><%= link_to 'Show', artist %></td>
    <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
    <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Artist', new_artist_path %>
