<h1>Listing genres</h1>

<table>
  <tr>
    <th>Genre release id</th>
    <th>Genre type id</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @genres.each do |genre| %>
  <tr>
    <td><%= genre.Genre_Release_ID %></td>
    <td><%= genre.Genre_Type_ID %></td>
    <td><%= genre.Created %></td>
    <td><%= genre.Updated %></td>
    <td><%= genre.Created_Person_ID %></td>
    <td><%= genre.Updated_Person_ID %></td>
    <td><%= link_to 'Show', genre %></td>
    <td><%= link_to 'Edit', edit_genre_path(genre) %></td>
    <td><%= link_to 'Destroy', genre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Genre', new_genre_path %>
