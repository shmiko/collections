<h1>Listing artist_types</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artist_types.each do |artist_type| %>
  <tr>
    <td><%= artist_type.Description %></td>
    <td><%= artist_type.Created %></td>
    <td><%= artist_type.Updated %></td>
    <td><%= artist_type.Created_Person_ID %></td>
    <td><%= artist_type.Updated_Person_ID %></td>
    <td><%= link_to 'Show', artist_type %></td>
    <td><%= link_to 'Edit', edit_artist_type_path(artist_type) %></td>
    <td><%= link_to 'Destroy', artist_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

