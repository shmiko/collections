<h1>Listing collection_types</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @collection_types.each do |collection_type| %>
  <tr>
    <td><%= collection_type.Description %></td>
    <td><%= collection_type.Created %></td>
    <td><%= collection_type.Updated %></td>
    <td><%= collection_type.Created_Person_ID %></td>
    <td><%= collection_type.Updated_Person_ID %></td>
    <td><%= link_to 'Show', collection_type %></td>
    <td><%= link_to 'Edit', edit_collection_type_path(collection_type) %></td>
    <td><%= link_to 'Destroy', collection_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

