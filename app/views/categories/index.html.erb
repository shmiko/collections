<h1>Listing categories</h1>

<table>
  <tr>
    <th>Category release id</th>
    <th>Category type id</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @categories.each do |category| %>
  <tr>
    <td><%= category.Category_Release_ID %></td>
    <td><%= category.Category_Type_ID %></td>
    <td><%= category.Created %></td>
    <td><%= category.Updated %></td>
    <td><%= category.Created_Person_ID %></td>
    <td><%= category.Updated_Person_ID %></td>
    <td><%= link_to 'Show', category %></td>
    <td><%= link_to 'Edit', edit_category_path(category) %></td>
    <td><%= link_to 'Destroy', category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Category', new_category_path %>
