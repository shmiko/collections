<h1>Listing category_types</h1>

<table>
  <tr>
    <th>Category type collection type id</th>
    <th>Description</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Created person id</th>
    <th>Updated person id</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @category_types.each do |category_type| %>
  <tr>
    <td><%= category_type.Category_Type_Collection_Type_ID %></td>
    <td><%= category_type.Description %></td>
    <td><%= category_type.Created %></td>
    <td><%= category_type.Updated %></td>
    <td><%= category_type.Created_Person_ID %></td>
    <td><%= category_type.Updated_Person_ID %></td>
    <td><%= link_to 'Show', category_type %></td>
    <td><%= link_to 'Edit', edit_category_type_path(category_type) %></td>
    <td><%= link_to 'Destroy', category_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

